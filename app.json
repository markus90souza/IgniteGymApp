{
  "expo": {
    "name": "igniteGym",
    "slug": "igniteGym",
    "version": "1.0.0",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#25292e"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
